<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.zzh.androidexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CommonViewActivity"></activity>

        <activity android:name=".activity.TabLayoutActivity"></activity>
        <activity android:name=".activity.NavigationViewActivity"></activity>
        <activity android:name=".activity.CoordinatorLayoutActivity"></activity>
        <activity android:name=".activity.RecyclerViewActivity"></activity>
        <activity android:name=".activity.RecyclerViewStaggerActivity"></activity>
        <activity android:name=".activity.FragmentActivity"></activity>
        <activity android:name=".activity.ListViewActivity"></activity>

    </application>

</manifest>
